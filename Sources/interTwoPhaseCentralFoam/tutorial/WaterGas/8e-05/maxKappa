FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "8e-05";
    object      maxKappa;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    sides
    {
        type            calculated;
        value           uniform 0;
    }
    empty1
    {
        type            empty;
    }
    empty2
    {
        type            empty;
    }
}


